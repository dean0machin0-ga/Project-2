<%- include('../partials/header') %>
<h2>2023/2024 Game Schedule</h2>
<table id="schedule">
    <thead>
        <tr>
            <th>Oppenent</th>
            <th>Game Date</th>
        </tr>
    </thead>
    <tbody>
        <% games.forEach(g => { %>
            <tr>
                <td><%= g.oppenent %></td>
                <td><%= g.gameDate %></td>
                <td><a href="/games/<%= g._id %>">DETAILS</a></td>
            </tr>
        <% }) %>
    </tbody>
</table>
<body>
    
</body>
</html>